{
    "manifest_version": 3,
    "name": "ReSolve.",
    "version": "1.0.0",
    "description": "A browser extension to keep track of unsolved problems on leetcode, codeforces and atcoder.",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["offscreen"],
    "content_security_policy": {
        "extension_pages": "script-src 'self' https://apis.google.com; object-src 'self'"
      }
  }